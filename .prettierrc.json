{
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"parser": "typescript",
	"printWidth": 90,
	"semi": true,
	"tabWidth": 2,
	"trailingComma": "all",
	"useTabs": true,
	"overrides": [
		{
			"files": "*.scss",
			"options": {
				"parser": "scss"
			}
		},
		{
			"files": "*.json",
			"options": {
				"parser": "json"
			}
		}
	]
}
